{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": false,
    "target": "ES6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/",
        "server/src/types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}